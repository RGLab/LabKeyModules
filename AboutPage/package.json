{
  "name": "aboutpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm install",
    "build": "npm run build-prod",
    "start": "cross-env NODE_ENV=development LK_MODULE=AboutPage webpack serve --config ../webpack/dev.config.js",
    "build-prod": "npm run clean && cross-env NODE_ENV=production LK_MODULE=AboutPage webpack --config ../webpack/prod.config.js",
    "clean": "rimraf resources/web/AboutPage/gen && rimraf resources/views",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@types/jquery": "^3.5.0",
    "bootstrap": "^3.4.1",
    "d3-color": "^1.4.0",
    "font-awesome": "4.7.0",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "cross-env": "5.2.1",
    "jest": "27.2.5",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "webpack": "5.22.0",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "4.3.1"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/test/styleMock.ts"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "eslintConfig": {
    "extends": [
      "../.eslintrc.json"
    ]
  }
}
