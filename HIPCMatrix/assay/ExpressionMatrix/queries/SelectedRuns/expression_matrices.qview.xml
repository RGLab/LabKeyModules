        <cv:column name='Name'>
            <cv:properties>
                <cv:property name='columnTitle' value='Name'/>
            </cv:properties>
        </cv:column>
        <cv:column name='Comments'/>
        <cv:column name='featureSet'/>
        <cv:column name='Cohort'/>
        <cv:column name='Folder/Name'>
            <cv:properties>
                <cv:property name='columnTitle' value='Study'/>
